{
	"hosting": [
		{
			"target": "exyleio-web",
			"public": "build",
			"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
			"rewrites": [
				{
					"source": "**",
					"destination": "/index.html"
				}
			]
		}
	],
	"emulators": {
		"auth": {
			"port": 9099
		},
		"hosting": {
			"port": 5000
		},
		"ui": {
			"enabled": true,
			"port": 4000
		}
	}
}
